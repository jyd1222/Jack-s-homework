#[w17-21-Q3]
def BubbleSort():
    bondery=99
    for i in range(99):
        for j in range(bondery):
            ThisID=UserNameArray[j]
            NextID=UserNameArray[j+1]
            if ThisID>NextID:
                temp=NextID
                NextID=ThisID
                ThisID=temp
            bondery-=1
def(FindRepeats):
    global UserNameArray
    count=0 
    for i in range(100):
        ThisUser=UserNameArray[i]
        NextUser=UserNameArray[i+1]
        if ThisUser[:6]==NextUser[:6]
        count+=1
        print(NextUser)
    if count=0:
        print('The array contains not repeated UserID')
    else:
        print('There are'),count,'repeated UserID')
    
    

